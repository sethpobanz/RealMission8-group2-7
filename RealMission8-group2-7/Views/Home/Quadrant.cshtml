@{
}
@{
    ViewData["Title"] = "Time Management Matrix";
}
@model List<TaskModel>

<div class="text-center">
    <h1 class="display-4">Quadrant page</h1>
</div>

<table class="table table-bordered">
    <thead class="fw-bold">
        <tr>
            <th>Task</th>
            <th>Due Date</th>
            <th>Quadrant</th>
            <th>Category</th>
            <th>Completed</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var task in Model)
        {
            <tr>
                <td>@task.task</td>
                <td>@task.duedate</td>
                <td>@task.quadrant</td>
                <td>@task.category</td>
                <td>@task.completed</td>
            </tr>
        }
    </tbody>
</table>
